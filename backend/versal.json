{
    "version": 2,
    "builds": [
      {
        "src": "frontend",
        "use": "@versal/vite-react" 
      },
      {
        "src": "backend", 
        "use": "@versal/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "backend/$1"
      },
      {
        "src": "/(.*)", 
        "dest": "frontend/$1"
      }
    ]
  }
  